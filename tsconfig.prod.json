{
  "compilerOptions": {
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "sourceMap": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es6",
      "dom"
    ],
    "exclude": [
      "node_modules/*",
      "assets/app/main.aot.ts"
    ],
    "angularCompilerOptions": {
      "genDir": "aot/",
      "skipMetadataEmit": true
    }
  }
}
